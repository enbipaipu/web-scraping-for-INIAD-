{
  "name": "web-scraping-for-iniad",
  "version": "1.0.0",
  "description": "INIAD#制作にあたり必要な関数をこの中に入っている。",
  "main": "index.js",
  "scripts": {
    "dev": "npm run migrate:deploy --prefix server && run-p dev:*",
    "dev:client": "next dev -p 3000",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "dev:typecheckClient": "tsc --noEmit -w",
    "dev:typecheckServer": "tsc --noEmit -p server -w",
    "build": "run-p build:client build:server",
    "prebuild:client": "npm run generate",
    "build:client": "next build && next export",
    "build:server": "npm run build --prefix server",
    "generate": "run-p generate:*",
    "generate:aspida": "aspida",
    "lint": "run-p lint:js lint:prettier lint:style",
    "lint:js": "eslint --ext .ts,.tsx,.js --ignore-path .gitignore .",
    "lint:prettier": "prettier --check \"./**/*.{ts,tsx,js}\" --ignore-path .gitignore",
    "lint:fix": "npm run lint:js -- --fix && prettier --write \"./**/*.{ts,tsx,js}\" --ignore-path .gitignore && npm run lint:style -- --fix",
    "pretypecheck": "npm run generate && npm run generate --prefix server",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:updateSnapshot": "vitest -u",
    "typecheck": "npm run hcm && tsc --noEmit && tsc --noEmit -p server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aspida/axios": "^1.14.0",
    "axios": "^1.6.2",
    "next": "^14.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.1",
    "@types/node-fetch": "^2.6.9",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  }
}
